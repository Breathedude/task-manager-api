<html>
<head>
    <meta charset='utf-8'>
    <meta http-equiv='X-UA-Compatible' content='IE=edge'>
    <title>Page Title</title>
    <meta name='viewport' content='width=device-width, initial-scale=1'>
    <link rel='stylesheet' type='text/css' media='screen' href='/css/main.css'>
    
</head>
<body>
    {{>header}}
    Transactions
   
     {{!-- <script src='../../src/index.js'></script> --}}
    <table>
        <thead>
            
            <th>Book Id</th>
            <th>Book name</th>
            <th>User name</th>
            <th>Borrowed time</th>
            <th>Return time</th>
        </thead>
        <tbody>
            {{#each transaction}}
            <tr>
                <td>{{book_id}}</td>
                <td>{{book_name}}</td>
                <td>{{user_name}}</td>
                <td>{{borrowed_time}}</td>
                <td>{{return_time}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
    <script src = '../../src/app.js'></script>
</html>